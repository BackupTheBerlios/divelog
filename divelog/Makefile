####### Build rules


all: $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

moc: $(SRCMOC)

tmake: Makefile

Makefile: divelog.pro
	tmake divelog.pro -o Makefile

dist:
	$(TAR) divelog.tar divelog.pro $(SOURCES) $(HEADERS) $(INTERFACES) $(DIST)
	$(GZIP) divelog.tar

clean:
